{
  "name": "fe_boilerplate",
  "version": "1.0.0",
  "private": false,
  "dependencies": {
    "@material-ui/core": "^4.9.9",
    "@material-ui/icons": "^4.9.1",
    "axios": "0.19.0",
    "cross-env": "^7.0.2",
    "firebase": "^7.13.2",
    "moment": "2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "5.2.1",
    "react-hot-loader": "^4.12.20",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.4.1",
    "styled-components": "5.0.0"
  },
  "devDependencies": {
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-jsx": "6.0.2",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jest": "22.6.4",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-standard": "4.0.0",
    "husky": "0.14.3",
    "prettier": "1.19.1",
    "pretty-quick": "1.4.1"
  },
  "scripts": {
    "dev": "cross-env REACT_APP_API_ENV=development react-scripts start",
    "build:dev": "REACT_APP_API_ENV=development react-scripts build",
    "deploy:dev": "yarn build:dev && firebase deploy --project=*****",
    "format": "prettier \"./src/**/*.js\" --write",
    "format:staged": "pretty-quick --staged",
    "precommit": "yarn format:staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
